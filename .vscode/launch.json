    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Flask",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "wsgi:app",
                "APP_ENV": "development",
                "FLASK_DEBUG": "1"
            },
            "args": [
                "run"
            ],
            "jinja": true
        },
        {
            "name": "Scraper",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "wsgi:app",
                "APP_ENV": "development",
                "FLASK_DEBUG": "1"
            },
            "args": [
                "run-scraper"
            ],
            "jinja": true
        },
        {
            "name": "Scrap States",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "wsgi:app",
                "APP_ENV": "development",
                "FLASK_DEBUG": "1"
            },
            "args": [
                "run-scrap-states"
            ],
            "jinja": true
        }
    ]
}